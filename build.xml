<?xml version="1.0"?>
<project name="test Project" default="phpunit">
  <property name="sitename" value="www.tutorialspoint.com"/>
  <property name="basedir" value="."/>

  <target name="phpunit" depends="process-build-resources-mysqli">
        <echo>MYSQLI unit tests started</echo>

        <exec executable="php" dir="${basedir}/test" failonerror="true" failifexecutionfails="true">
        </exec>

        <echo>MYSQLI unit tests finished</echo>
    </target>
</project>


